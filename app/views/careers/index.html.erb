<h2 class="center-align">Careers</h2>
<hr>
<button class="btn" id="toggle">Add new career</button>
<div class="row">
  <div class="col s12 m6">
    <div class="card">
      <div class="card-content white-text">
        <ul id="career-list">
          <% @careers.each do |career| %>
            <%= render partial: 'career', locals: { career: career } %>
            <br>
          <% end %>
        </ul>
      </div>
    </div>
  </div>

  <div class="col s12 m6">
    <div class="card">
      <div class="card-content white-text">
        <h5 id="career" class="center-align">People who have this career</h5>
        <ul id="people">
        </ul>
      </div>
    </div>
  </div>
</div>
