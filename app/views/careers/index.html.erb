<h2 class="">Careers</h2>
<hr>
<div class="row">
  <div class="col s12 m6">
    <button class="btn" id="toggle">Add new career</button>
    <ul id="career-list">
      <% @careers.each do |career| %>
        <%= render partial: 'career', locals: { career: career } %>
      <% end %>
    </ul>
  </div>
  <div class="col s12 m6">
    <h4 id="career">People who have this career</h4>
    <ul id="people">
    </ul>
  </div>
</div>
